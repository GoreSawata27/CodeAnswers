 // java 
public class questionThree {
    public static void main(String[] args) {

        String str = "Aana";
        int length = str.length();
        String rev = "";

        for ( int i = length - 1; i >= 0; i-- )
            rev = rev + str.charAt(i);

        if (str.equals(rev))
            System.out.println("true");
        else
            System.out.println("false");

    }

    }

